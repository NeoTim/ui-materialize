{
  "name": "ui-materialize",
  "version": "0.0.1",
  "description": "AngularJS Directive Library for MaterializeCSS",
  "authors": [
    "JoelCox"
  ],
  "license": "MIT",
  "homepage": "ui-materialize.com",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "devDependencies": {
    "angular-mocks": "~1.3.8"
  },
  "dependencies": {
    "angular": "~1.3.8",
    "toastr": "~2.1.0",
    "moment": "~2.6.0",
    "jquery": "~2.1.0",
    "angular-route": "~1.3.8",
    "angular-animate": "~1.3.8",
    "angular-sanitize": "~1.3.8",
    "font-awesome": "~4.0.3",
    "extras.angular.plus": "~0.9.0",
    "gsap": "~1.15.0",
    "angular-ui-router": "~0.2.13",
    "lodash": "~2.4.1",
    "angular-scroll": "~0.6.4",
    "angular-markdown-directive": "~0.3.1",
    "jquery-slimscroll": "~1.3.3",
    "materialize": "~0.95.1"
  },
  "overrides": {
    "font-awesome": {
      "main": "css/font-awesome.css"
    },
    "toastr":{"main": ["toastr.js"]},
    "showdown": {
      "main": [
        "src/showdown.js",
        "compressed/extensions/github.min.js",
        "compressed/extensions/prettify.min.js",
        "compressed/extensions/table.min.js",
        "compressed/extensions/twitter.min.js"
      ]
    },
    "materialize": {
      "main": [
        "dist/css/materialize.css",
        "dist/js/materialize.js",
        "font/material-design-icons/Material-Design-Icons.eot",
        "font/material-design-icons/Material-Design-Icons.svg",
        "font/material-design-icons/Material-Design-Icons.ttf",
        "font/material-design-icons/Material-Design-Icons.woff",
        "font/roboto/Roboto-Bold.ttf",
        "font/roboto/Roboto-Light.ttf",
        "font/roboto/Roboto-Medium.ttf",
        "font/roboto/Roboto-Regular.ttf",
        "font/roboto/Roboto-Thin.ttf"
      ]
    }
  },
  "main": "gulpfile.js",
  "keywords": [
    "material",
    "material",
    "design",
    "angular",
    "directives",
    "ng",
    "gulp",
    "materialize"
  ]
}
